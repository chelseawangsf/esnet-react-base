{
  "name": "esnet-react-base",
  "version": "0.5.1",
  "description": "Common widgets for ESnet tools",
  "main": "index.js",
  "author": "ESnet Tools Team <tools@es.net>",
  "repository": "esnet/esnet-react-base",
  "homepage": "https://software.es.net/esnet-react-base",
  "bugs": {
    "url": "https://github.com/esnet/esnet-react-base/issues"
  },
  "scripts": {
    "prepublish": "npm test && npm run build",
    "lint": "eslint src/*.jsx",
    "test": "npm run lint",
    "test-only": "echo \"Error: no test specified\" && exit 1",
    "build": "rm -rf lib/* && cp src/*.css lib/ && cp src/*.gif lib/ && babel src --optional runtime --stage 0 --out-dir lib",
    "build-website": "webpack --config webpack.examples.config.js",
    "start-website": "webpack-dev-server  --config webpack.examples.config.js"
  },
  "pre-commit": [
    "lint",
    "build"
  ],
  "dependencies": {
    "babel-runtime": "~5.8.3",
    "moment-timezone": "^0.4.1",
    "react-bootstrap": "^0.28.1",
    "underscore": "^1.7.0"
  },
  "peerDependencies": {
    "react": "^0.14.3"
  },
  "devDependencies": {
    "babel": "^5.8.3",
    "babel-core": "^5.1.13",
    "babel-eslint": "^4.1.5",
    "babel-loader": "^5.0.0",
    "bundle-loader": "^0.5.0",
    "css-loader": "^0.9.0",
    "eslint": "^1.10.0",
    "eslint-plugin-react": "^3.9.0",
    "eslint-plugin-babel": "^2.1.1",
    "eslint-config-esnet": "^0.1.0",
    "file-loader": "^0.7.2",
    "history": "^1.13.1",
    "json-loader": "^0.5.1",
    "raw-loader": "^0.5.1",
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "react-markdown": "^1.0.5",
    "react-router": "^1.0.0",
    "scroll-behavior": "^0.3.0",
    "style-loader": "^0.8.0",
    "url-loader": "^0.5.5",
    "webpack-dev-server": "^1.6.5"
  }
}
