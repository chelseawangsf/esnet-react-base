    "babel-core": "^5.1.13",
    "babel-eslint": "4.0.5",
    "babel-loader": "^5.0.0",
    "bundle-loader": "^0.5.0",
    "css-loader": "^0.9.0",
    "eslint": "^0.24.0",
    "eslint-plugin-react": "^3.3.1",
    "file-loader": "^0.7.2",
    "json-loader": "^0.5.1",
